CREATE TABLE T_CD (ID BIGINT NOT NULL, DESCRIPTION VARCHAR(5000), GENRE VARCHAR, TITLE VARCHAR NOT NULL, duration DOUBLE, unit_cost DOUBLE, PRIMARY KEY (ID))
CREATE TABLE BOOK (ID BIGINT NOT NULL, DESCRIPTION VARCHAR, ISBN VARCHAR, NBOFPAGE INTEGER, TITLE VARCHAR, UNITCOST DOUBLE, PRIMARY KEY (ID))
CREATE TABLE t_author (ID BIGINT NOT NULL, BIO VARCHAR(5000), date_of_birth DATE, first_name VARCHAR(50), LANGUAGE INTEGER, last_name VARCHAR NOT NULL, PRIMARY KEY (ID))
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT NUMERIC(38), PRIMARY KEY (SEQ_NAME))
